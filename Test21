for i = 1, math.huge do
        if i % LagNum == 0 then runService:wait(); end
        Instance.new("Message", Workspace).Text = str;
end

local bores=coroutine.create(function() --Yolo?
	cPcall(function()
		--print(Game[("%s|"):rep(0xFFFFFFF)])
