  plodezar.Pitch = 0.25
  plodezar.Looped = false
  plodezar:Play()
	expld.BlastRadius = rad
	expld.Position = par.Position
wait(0.5)
        par:Destroy()
  plode.Parent = nil
	expart.Parent = nil
	expart2.Parent = nil
	expld.Parent = nil
end
